ul(id='brotherdropdown' class='dropdown-content')
	li
		a(href='/brothers?who=all' class='blue-text') All
	li(class='template' style="display: none")
		a(href="/brothers?who=class" class="blue-text light") text
nav(class='bg-dark text-white' role='navigation')
	div(class='nav-wrapper container')
		a(class='brand-logo' href='/' id='logo-container')
			img(src='media/general/crest_small.png' vspace='15')
		ul(class='right hide-on-med-and-down')
			li
				a(href="/" class="white-text text-lighten-2" style="text-decoration: none;") 
					span HOME
			li(class='dropdown' onmouseover='showDropdown(1)' onmouseout='hideDropdown(1)')
				a(href="/#who-are-we" class="white-text text-lighten-2" style="text-decoration: none;") 
					span ABOUT
						.dropdowncontent
							a(href='/about' style="text-decoration: none;") 
								span Our Chapter
							a(href='/about/thetatau' style="text-decoration: none;") 
								span Theta Tau
							a(href='/about/greeklife' style="text-decoration: none;") 
								span GreekLife
			li(class='dropdown' onmouseover='showDropdown(2)' onmouseout='hideDropdown(2)')
				a(href="/#events" class="link nav-events white-text text-lighten-2" style="text-decoration: none;") 
					span EVENTS
						.dropdowncontent
							a(href='/events/brotherhood' style="text-decoration: none;") 
								span Brotherhood
							a(href='/events/professional' style="text-decoration: none;") 
								span Professional
							a(href='/events/service' style="text-decoration: none;") 
								span Service
							a(href='/events/annual' style="text-decoration: none;") 
								span Annual
			li(class='dropdown' onmouseover='showDropdown(3)' onmouseout='hideDropdown(3)')
				a(href="/#spotlight" class="white-text text-lighten-2" style="text-decoration: none;") 
					span SPOTLIGHT
						.dropdowncontent
							a(href='/spotlight' style="text-decoration: none;") 
								span All
			li
				a(href='/rush' class="white-text text-lighten-2" style="text-decoration: none;") 
					span RUSH
			li(class='dropdown' onmouseover='showDropdown(3)' onmouseout='hideDropdown(3)')
				a(class="dropdown-button white-text text-lighten-2" href="/#brothers" style="text-decoration: none;")
					span BROTHERS
						.dropdowncontent
							a(href='/brothers?who=all' style="text-decoration: none;") 
								span All
		ul(class="side-nav" id="nav-mobile" style="left: -250px;")
			li.active
				a(href="/" class="white-text text-lighten-2" style="text-decoration: none;") 
					span HOME
			li
				a(href="/rush" class="white-text light" style="text-decoration: none;") 
					span RUSH
			li
				a(href="/brothers?who=all" class="white-text light" style="text-decoration: none;")
		a(class="button-collapse white-text" data-activates="nav-mobile" href="#" style="text-decoration: none;")
			i(class="material-icons") menu
		
					
				
		